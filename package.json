{
  "name": "crud-project",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run prod",
    "server": "babel-node ./src/bin/www",
    "server:dev": "nodemon ./src/bin/www",
    "server:prod": "node ./dist/bin/www",
    "dev": "SET NODE_ENV=development & npm-run-all server",
    "clean": "rimraf dist",
    "build": "babel ./src --out-dir dist",
    "prod": "SET NODE_ENV=production & npm-run-all clean build server:prod",
    "watch": "nodemon"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "morgan": "~1.9.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "bcrypt": "^5.0.0",
    "express-validator": "^6.1.0",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^1.6.5",
    "nodemon": "^1.19.1",
    "sequelize": "^5.8.12",
    "sequelize-mysql": "^1.7.0",
    "url": "^0.11.0"
  }
}
